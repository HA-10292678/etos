<?xml version="1.0" encoding="UTF-8"?>

<model>
	<transaction id="starter">
		<pause_to>
			<time>1d8:00</time>
		</pause_to>
		<start_transaction>
			<pause><duration>120:00</duration></pause>
			<trace text="simulation stopped"/>
			<stop_simulation/>
		</start_transaction>
		<counted_loop>
			<count>2</count>
			<transaction>
				<start_transaction transactionUrl="#transaction[@id='route']"/>
			</transaction>
		</counted_loop>
	</transaction>
	<transaction id="route" entities="#entities" actor="#actor">
		<counted_loop>
			<count>3</count>
			<transaction>
				<pause_to period="24:00">
					<time>2:00:00</time>
				</pause_to>
				<checkpoint>
					<measure property="t.id" category="transaction" type="log"/>
					<measure property="a.b_energy" category="energy" type="log"/>

				</checkpoint>
				<pause><duration>1</duration></pause>
				<with>
					<probability>0.5</probability>
					<transaction>
						<checkpoint>
							<measure property="t.id" category="exit!!"
								 type="log"/>
						</checkpoint>
						<exit/>
					</transaction>
				</with>
			</transaction>
		</counted_loop>
	</transaction>
	<entities>
	</entities>
	<actor>
		<b_capacity>1.44e+08</b_capacity>
		<b_consumption>960</b_consumption>
		<b_energy>1.44e+08</b_energy>
	</actor>
</model>
