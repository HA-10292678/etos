<?xml version="1.0" encoding="UTF-8"?>

<model>
<transaction id="starter">
	<start_transaction transactionUrl="#transaction[@id='gastrans']"/>
	<start_transaction transactionUrl="#transaction[@id='gastrans']"/>
        <start_transaction transactionUrl="#transaction[@id='gastrans']"/>
	<start_transaction transactionUrl="#transaction[@id='gastrans']"/>
        <checkpoint referedEntity="prev">
                <measure property="s.t" category="trans started" type="log"></measure>
        </checkpoint>

</transaction>
<transaction id="gastrans" entities="#entities">
    <infinity_loop>
        <transaction>          
            <pause><duration><pnormal mu="120" sigma="1"/></duration></pause>
            <rtank id="TANK1">
                <amount><pnormal mu="0.5" sigma="0.3"/></amount>
            </rtank>
            <checkpoint referedEntity="prev">
                <measure property="e.refuel" category="end" type="log" key="t.id"/>
		<measure property="e.realAmount" category="end" type="log" key="t.id"/>
            </checkpoint>
        </transaction>
    </infinity_loop>
</transaction>
<entities>
     <rtank id="TANK1">
        <capacity>100</capacity>
        <initialAmount>10</initialAmount>
     </rtank>
</entities>
</model>
